      <viewAttribute id="DATE"/>
      <viewAttribute id="HC" aggregationType="sum"/>
      <viewAttribute id="SAL" aggregationType="sum"/>
    </viewAttributes>
    <calculatedViewAttributes/>
    <input node="Projection_1"/>
    <rankingFilter type="RANK" offset="0" offsetReference="BACK" dynamicPartitionAttributes="false">
      <partitionViewAttributeName id="EMP_NO"/>
      <partitionViewAttributeName id="NAME"/>
      <order byViewAttributeName="DATE" direction="ASC"/>
      <rankThreshold>
        <value>1</value>
      </rankThreshold>
      <rankViewAttributeName>Rank_Column</rankViewAttributeName>
    </rankingFilter>
  </calculationView>
