      <mapping xsi:type="Calculation:AttributeMapping" target="STATE" source="STATE"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="EMP_NO" source="EMP_NO"/>
    </input>
    <joinAttribute name="EMP_NO"/>
  </calculationView>
